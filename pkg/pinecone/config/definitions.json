[
  {
    "uid": "4b1dcf82-e134-4ba7-992f-f9a02536ec2b",
    "id": "data-pinecone",
    "title": "Pinecone",
    "documentation_url": "https://www.instill.tech/docs/vdp/data-connectors/pinecone",
    "icon": "pinecone.svg",
    "icon_url": "https://www.pinecone.io/favicon.ico",
    "public": true,
    "custom": false,
    "tombstone": false,
    "spec": {
      "resource_specification": {
        "$schema": "http://json-schema.org/draft-07/schema#",
        "title": "Pinecone Connector Spec",
        "type": "object",
        "required": [
          "api_key",
          "url"
        ],
        "additionalProperties": false,
        "properties": {
          "api_key": {
            "title": "API Key",
            "description": "Fill your Stability AI API key. To find your keys, navigate to your DreamStudio's Account page.",
            "type": "string",
            "credential_field": true
          },
          "url": {
            "title": "Pinecone Base URL",
            "description": "Fill in your Pinecone base URL. IT is of the form [https://index_name-project_id.svc.environment.pinecone.io]",
            "type": "string",
            "credential_field": false
          }
        }
      },
      "component_specification": {
        "$schema": "http://json-schema.org/draft-07/schema#",
        "title": "Pinecone Component Setting",
        "type": "object",
        "properties": {
          "input": {
            "properties": {
              "task": {
                "title": "Task",
                "enum": [
                  "QUERY",
                  "UPSERT"
                ]
              }
            }
          }
        },
        "allOf": [
          {
            "if": {
              "properties": {
                "input": {
                  "properties": {
                    "task": {
                      "const": "QUERY"
                    }
                  }
                }
              }
            },
            "then": {
              "properties": {
                "metadata": {
                  "title": "Metadata",
                  "type": "object"
                },
                "input": {
                  "type": "object",
                  "required": [
                    "topK"
                  ],
                  "properties": {
                    "namespace": {
                      "description": "The namespace to query",
                      "instillFormat": "text",
                      "instillUpstreamTypes": [
                        "value",
                        "reference"
                      ],
                      "oneOf": [
                        {
                          "type": "string",
                          "instillUpstreamType": "value"
                        },
                        {
                          "type": "string",
                          "pattern": "^\\{.*\\}$",
                          "instillUpstreamType": "reference"
                        }
                      ]
                    },
                    "topK": {
                      "description": "The number of results to return for each query",
                      "instillFormat": "integer",
                      "instillUpstreamTypes": [
                        "value",
                        "reference"
                      ],
                      "oneOf": [
                        {
                          "type": "integer",
                          "instillUpstreamType": "value"
                        },
                        {
                          "type": "string",
                          "pattern": "^\\{.*\\}$",
                          "instillUpstreamType": "reference"
                        }
                      ]
                    },
                    "includeValues": {
                      "description": "Indicates whether vector values are included in the response",
                      "instillFormat": "boolean",
                      "instillUpstreamTypes": [
                        "value",
                        "reference"
                      ],
                      "oneOf": [
                        {
                          "type": "boolean",
                          "default": false,
                          "instillUpstreamType": "value"
                        },
                        {
                          "type": "string",
                          "pattern": "^\\{.*\\}$",
                          "instillUpstreamType": "reference"
                        }
                      ]
                    },
                    "includeMetadata": {
                      "description": "Indicates whether metadata is included in the response as well as the ids",
                      "instillFormat": "boolean",
                      "instillUpstreamTypes": [
                        "value",
                        "reference"
                      ],
                      "oneOf": [
                        {
                          "type": "boolean",
                          "default": false,
                          "instillUpstreamType": "value"
                        },
                        {
                          "type": "string",
                          "pattern": "^\\{.*\\}$",
                          "instillUpstreamType": "reference"
                        }
                      ]
                    },
                    "vectors": {
                      "description": "An array of dimensions for the query vector.",
                      "instillFormat": "number_array",
                      "instillUpstreamTypes": [
                        "reference"
                      ],
                      "oneOf": [
                        {
                          "type": "string",
                          "pattern": "^\\{.*\\}$",
                          "instillUpstreamType": "reference"
                        }
                      ]
                    },
                    "id": {
                      "description": "The unique ID of the vector to be used as a query vector",
                      "instillFormat": "text",
                      "instillUpstreamTypes": [
                        "reference"
                      ],
                      "oneOf": [
                        {
                          "type": "string",
                          "pattern": "^\\{.*\\}$",
                          "instillUpstreamType": "reference"
                        }
                      ]
                    }
                  },
                  "title": "Data Flow"
                }
              }
            }
          },
          {
            "if": {
              "properties": {
                "input": {
                  "properties": {
                    "task": {
                      "const": "UPSERT"
                    }
                  }
                }
              }
            },
            "then": {
              "properties": {
                "metadata": {
                  "title": "Metadata",
                  "type": "object"
                },
                "input": {
                  "type": "object",
                  "required": [
                    "vectors"
                  ],
                  "properties": {
                    "vectors": {
                      "type": "array",
                      "description": "An array of vectors to be saved",
                      "items": {
                        "type": "object",
                        "required": [
                          "id",
                          "values"
                        ],
                        "properties": {
                          "id": {
                            "type": "string",
                            "description": "This is the vector's unique id",
                            "instillFormat": "text",
                            "instillUpstreamTypes": [
                              "reference"
                            ]
                          },
                          "values": {
                            "type": "array",
                            "description": "An array of dimensions for the vector to be saved",
                            "items": {
                              "type": "number",
                              "description": "A dimension of the vector",
                              "example": 0.8167237
                            },
                            "minItems": 1,
                            "instillFormat": "number_array",
                            "instillUpstreamTypes": [
                              "reference"
                            ]
                          }
                        }
                      }
                    }
                  },
                  "title": "Data Flow"
                }
              }
            }
          }
        ]
      },
      "openapi_specifications": {
        "QUERY": {
          "openapi": "3.0.0",
          "info": {
            "version": "1.0.0",
            "title": "Pinecone Connector OpenAPI"
          },
          "paths": {
            "/execute": {
              "post": {
                "requestBody": {
                  "required": true,
                  "content": {
                    "application/json": {
                      "schema": {
                        "type": "object",
                        "properties": {
                          "inputs": {
                            "type": "array",
                            "items": {
                              "type": "object",
                              "required": [
                                "topK"
                              ],
                              "properties": {
                                "namespace": {
                                  "type": "string",
                                  "description": "The namespace to query",
                                  "instillFormat": "text",
                                  "instillUpstreamTypes": [
                                    "value",
                                    "reference"
                                  ]
                                },
                                "topK": {
                                  "type": "integer",
                                  "description": "The number of results to return for each query",
                                  "instillFormat": "integer",
                                  "instillUpstreamTypes": [
                                    "value",
                                    "reference"
                                  ]
                                },
                                "includeValues": {
                                  "type": "boolean",
                                  "description": "Indicates whether vector values are included in the response",
                                  "default": false,
                                  "instillFormat": "boolean",
                                  "instillUpstreamTypes": [
                                    "value",
                                    "reference"
                                  ]
                                },
                                "includeMetadata": {
                                  "type": "boolean",
                                  "description": "Indicates whether metadata is included in the response as well as the ids",
                                  "default": false,
                                  "instillFormat": "boolean",
                                  "instillUpstreamTypes": [
                                    "value",
                                    "reference"
                                  ]
                                },
                                "vectors": {
                                  "type": "array",
                                  "description": "An array of dimensions for the query vector.",
                                  "items": {
                                    "type": "number",
                                    "description": "A dimension of the vector",
                                    "example": 0.8167237
                                  },
                                  "minItems": 1,
                                  "instillFormat": "number_array",
                                  "instillUpstreamTypes": [
                                    "reference"
                                  ]
                                },
                                "id": {
                                  "type": "string",
                                  "description": "The unique ID of the vector to be used as a query vector",
                                  "instillFormat": "text",
                                  "instillUpstreamTypes": [
                                    "reference"
                                  ]
                                }
                              }
                            }
                          }
                        }
                      }
                    }
                  }
                },
                "responses": {
                  "200": {
                    "description": "",
                    "content": {
                      "application/json": {
                        "schema": {
                          "type": "object",
                          "properties": {
                            "outputs": {
                              "type": "array",
                              "items": {
                                "type": "object",
                                "properties": {
                                  "namespace": {
                                    "type": "string",
                                    "description": "The namespace of the query",
                                    "instillFormat": "text"
                                  },
                                  "matches": {
                                    "type": "array",
                                    "description": "The matches returned for the query",
                                    "items": {
                                      "type": "object",
                                      "properties": {
                                        "id": {
                                          "type": "string",
                                          "description": "The id of the matched vector",
                                          "instillFormat": "text"
                                        },
                                        "score": {
                                          "type": "number",
                                          "description": "A measure of similarity between this vector and the query vector. The higher the score, the more they are similar.",
                                          "instillFormat": "number"
                                        },
                                        "values": {
                                          "type": "array",
                                          "description": "Vector data values",
                                          "items": {
                                            "type": "number",
                                            "description": "Each float value represents one dimension"
                                          },
                                          "instillFormat": "number_array"
                                        }
                                      }
                                    }
                                  }
                                }
                              }
                            }
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        },
        "UPSERT": {
          "openapi": "3.0.0",
          "info": {
            "version": "1.0.0",
            "title": "Pinecone Connector OpenAPI"
          },
          "paths": {
            "/execute": {
              "post": {
                "requestBody": {
                  "required": true,
                  "content": {
                    "application/json": {
                      "schema": {
                        "type": "object",
                        "properties": {
                          "inputs": {
                            "type": "array",
                            "items": {
                              "type": "object",
                              "required": [
                                "vectors"
                              ],
                              "properties": {
                                "vectors": {
                                  "type": "array",
                                  "description": "An array of vectors to be saved",
                                  "items": {
                                    "type": "object",
                                    "required": [
                                      "id",
                                      "values"
                                    ],
                                    "properties": {
                                      "id": {
                                        "type": "string",
                                        "description": "This is the vector's unique id",
                                        "instillFormat": "text",
                                        "instillUpstreamTypes": [
                                          "reference"
                                        ]
                                      },
                                      "values": {
                                        "type": "array",
                                        "description": "An array of dimensions for the vector to be saved",
                                        "items": {
                                          "type": "number",
                                          "description": "A dimension of the vector",
                                          "example": 0.8167237
                                        },
                                        "minItems": 1,
                                        "instillFormat": "number_array",
                                        "instillUpstreamTypes": [
                                          "reference"
                                        ]
                                      }
                                    }
                                  }
                                }
                              }
                            }
                          }
                        }
                      }
                    }
                  }
                },
                "responses": {
                  "200": {
                    "description": "",
                    "content": {
                      "application/json": {
                        "schema": {
                          "type": "object",
                          "properties": {
                            "outputs": {
                              "type": "array",
                              "items": {
                                "type": "object",
                                "properties": {
                                  "upsertedCount": {
                                    "type": "integer",
                                    "description": "Number of records modified or added",
                                    "instillFormat": "integer"
                                  }
                                }
                              }
                            }
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
