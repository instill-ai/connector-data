{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "metadata": {
      "type": "object",
      "patternProperties": {
        "^[a-zA-Z_][a-zA-Z_0-9]*$": {
          "type": "object",
          "properties": {
            "title": {
              "title": "Title",
              "type": "string"
            }
          }
        }
      }
    },
    "input": {
      "type": "object",
      "patternProperties": {
        "^[a-zA-Z_][a-zA-Z_0-9]*$": {
          "type": "string",
          "pattern": "^\\{.*\\}$"
        }
      }
    }
  }
}
